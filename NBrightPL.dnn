<dotnetnuke type="Package" version="5.0">
	<packages>
		<package name="NBrightPL" type="Module" version="1.5.0">
			<friendlyName>NBrightPL</friendlyName>
			<description>NBrightPL is a module to support Page Localization in DNN without having to enable full content localization.</description>
			<iconFile />
			<owner>
				<name>David Lee</name>
				<organization>Nevoweb</organization>
				<url>www.nevoweb.com</url>
				<email>info@nevoweb.com</email>
			</owner>
